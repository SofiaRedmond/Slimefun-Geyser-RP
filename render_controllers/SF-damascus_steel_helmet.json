{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.damascus_steel_helmet": {
			"textures": [
				"texture.default",
				"Texture.enchanted"
			],
			"geometry": "query.is_item_name_any('slot.armor.head','1','geyser_custom:damascus_steel_helmet') ? geometry.default : geometry.none",
			"materials": [
				{
					"*": "variable.is_enchanted ? Material.enchanted : Material.default"
				}
			]
		}
	}
}